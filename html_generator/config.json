    "title": "Aymen's Power \nManagement Server",
    "target": "http://127.0.0.1:5000/",
    
    "form_elements": [
        {
            "header": {
                "header_text": "Password:"
            }
        },
        {
            "password": {
                "input_name": "password",
                "placeholder": "password"
            }
        },
        {
            "header": {
                "header_text": "Options:"
            }
        },
        {
            "grouped_radioMaster": {
                "input_name": "mode",
                "radio_options": [
                    {
                        "option_label": "Turn On",
                        "option_value": "on"
                    },
                    {
                        "option_label": "Turn Off",
                        "option_value": "off"
                    }
                ]
            }
        },
        {
            "grouped_selectMaster": {
                "input_name": "cars",
                "select_options": [
                    {
                        "option_label": "Volvo",
                        "option_value": "volvo"
                    },
                    {
                        "option_label": "Mercedes",
                        "option_value": "mercedes"
                    }
                ]
            }
        },
        {
            "submit": {
                "value": "Send",
                "font_size": "1.2rem",
                "font_weight": 600
            }
        }        
    ]
}